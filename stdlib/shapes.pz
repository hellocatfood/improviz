
func myBox(x, y, z)
  x := 1
  y := x
  z := y
  shape(:cube, x, y, z)

func mySphere(x, y, z)
  x := 1
  y := x
  z := y
  shape(:sphere, x, y, z)

func myCylinder(x, y, z)
  x := 1
  y := x
  z := y
  shape(:cylinder, x, y, z)

func myRectangle(x, y)
  x := 1
  y := x
  shape(:rectangle, x, y, z)

func myLine(x)
  x := 1
  shape(:line, x)

